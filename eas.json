{
  "cli": {
    "version": ">= 10.1.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "extends": "base",
      "autoIncrement": true,
      "node": "20.11.1",
      "npm": "10.2.4",
      "android": {
        "env": {
          "PLATFORM": "android"
        }
      },
      "ios": {
        "env": {
          "PLATFORM": "ios"
        }
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "path/to/serviceAccountKey.json",
        "track": "internal"
      }
    }
  }
}
